{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/scraped_data/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|svg|webp|pdf|ico))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/login.html"
    }
  ]
}

